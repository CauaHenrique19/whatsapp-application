model User {
  id          Int       @id @default(autoincrement())
  email       String    @unique
  name        String    
  last_name   String    
  password    String
  admin       Boolean   @default(false)
  client_id   Int       
  status      Int
  createdAt   DateTime    @default(now())

  client      Client    @relation(fields: [client_id], references: [id])

  @@map("tb_user")
}
